import {
Entity,
Column,
PrimaryGeneratedColumn,
CreateDateColumn,
UpdateDateColumn,
} from 'typeorm';

@Entity('<%= tableName %>')
export class <%= moduleName %> {
@PrimaryGeneratedColumn()
id: number;

@Column({default: 0, comment: '用户id'})
user_id: number;

@Column({comment: '类型名称'})
name: string;

@CreateDateColumn()
created_at: Date;

@UpdateDateColumn()
updated_at: Date;
}
